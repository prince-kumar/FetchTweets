   pod 'TwitterKit'
   pod 'TwitterCore'


  # Pods for FetchTweets

  target 'FetchTweetsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FetchTweetsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
